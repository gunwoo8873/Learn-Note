.PHONY: \
	arithemtic comparison logical \
	datatype pointer instance \
	closures vaiabic \
	array linked collecter map switch \
	if for case switch \
	io time \

# Standard
datatype: 
	go run ./datatype/main.go

# Operator
arithemtic: # 계산 연산자
	go run ./Operator/Arithemtic/main.go

comparison: # 쉬프트
	go run ./Operator/Comparison/main.go

logical: # 논리 연산자
	go run ./Operator/Logical/main.go
#########################################################

pointer: # 포인터
	go run ./Pointer/main.go

instance: # 인스턴스
	go run ./Instance/main.go
#########################################################

# Function
closures: 
	go run ./Closures/main.go

vaiabic: 
	go run ./Function/Vaiabic/main.go
#########################################################

#Container
array: 
	go run ./Container/Array/main.go

linked: 
	go run ./Container/Linked/main.go

collecter: 
	go run ./Container/Collecter/main.go

map: 
	go run ./Container/Map/main.go
#########################################################

# Controlflow
if: 
	go run ./Controlflow/If/main.go

for: 
	go run ./Controlflow/For/main.go

case: 
	go run ./Controlflow/Case_Goto/main.go

switch: 
	go run ./Controlflow/Switch/main.go
#########################################################

# Utility
io: 
	go run ./Interface/IO/main.go

time: 
	go run ./Time/main.go
#########################################################